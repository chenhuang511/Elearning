{% extends "layouts/layout-dashboard.twig" %}
            {% block page %}
                {% set page = page | merge({
                "title"       : box_title,
                "description" : "Mdluser management, CRUD mdlUser"
                }) %}
                {{ parent() }}
            {% endblock %}

            {% block content %}
                <div class="row">
                    <div class="col-lg-12">
                        <div class="panel panel-primary">
                            <div class="panel-heading">
                                <h3 class="panel-title"><i class="fa fa-users"></i> {{ box_title }}</h3>
                            </div>

                            <div class="panel-body">
                                <form class="form-horizontal" enctype="multipart/form-data" role="form" name="mdlusercreate" action="{{ form_action }}" method="post" id="id_mldusercreate">
                                    {#General collapse#}
                                    <div class="form-group">
                                        <div class="panel-group">
                                            <div class="panel panel-default">
                                                <div class="panel-heading">
                                                    <h4 class="panel-title">
                                                        <a data-toggle="collapse" href="#collapse_Gen">General</a>
                                                    </h4>
                                                </div>
                                                <div id="collapse_Gen" class="panel-collapse collapse in">
                                                    <ul class="list-group">
                                                        <li class="list-group-item">
                                                            <div class="form-group">
                                                                <label for="id_username" class="col-sm-4 control-label">Username<span style='color:#d11010; font-size: large'>*</span> </label>
                                                                <div class="col-ms-5" style="display: table; width:40%">
                                                                    {#<span class="input-group-addon"><i class="fa fa-edit"></i></span>#}
                                                                    <input type="text" class="form-control" style="margin-right: 15px" name="username" id="id_username" autocomplete="off"
                                                                           value="{{mdluser.username}}" placeholder="Please enter the user name">
                                                                </div>
                                                            </div>
                                                        </li>

                                                        <li class="list-group-item">
                                                            <div class="form-group">
                                                                <label for="id_auth" class="col-sm-4 control-label">Choose an authentication method</label>
                                                                <div class="input-group" style="width: 40%">
                                                                    <span class="input-group-addon"><i class="fa fa-language"></i></span>
                                                                    <select id="id_auth" class='form-control select2' name="auth">
                                                                        {% for option, label in auths %}
                                                                            <option value="{{option}}" {% if (option == mdluser.auth) %}selected{% endif %}>{{label}}</option>
                                                                        {% endfor %}
                                                                    </select>
                                                                </div>
                                                            </div>
                                                        </li>
                                                        <li class="list-group-item">
                                                            <div class="form-group">
                                                                <label for="id_suspended" class="col-sm-4 control-label">Suspended account</label>
                                                                <input type="checkbox" style="margin: 10px 0 0"  name="suspended" id ="id_suspended" {% if mdluser.suspended %}checked{% endif %}>
                                                            </div>
                                                        </li>
                                                        <li class="list-group-item">
                                                            <div class="form-group">
                                                                <label for="id_createpassword" class="col-sm-4 control-label">Generate password and notify user</label>
                                                                <input type="checkbox" style="margin: 10px 0 0" name="createpassword" id ="id_createpassword" >
                                                            </div>
                                                        </li>
                                                        <li class="list-group-item">
                                                            <div class="form-group">
                                                                <label for="id_password" class="col-sm-4 control-label">New password</label>
                                                                <div class="col-ms-5" style="display: table; width:40%">
                                                                    <div class="input-group">
                                                                        <label class="input-group-addon"><i class="fa fa-edit"></i></label>
                                                                        <input type="password" name="password" class="form-control" style="width: 60%; margin-right: 15px; border-radius:4px" value="" placeholder="Enter Password" id="id_password">
                                                                        <button class="unmask" type="button" title="Mask/Unmask password to check content" style="margin: 4px 2px; border-radius:4px">Unmask</button>
                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </li>
                                                        <li class="list-group-item">
                                                            <div class="form-group">
                                                                <label for="id_preference_auth_forcepasswordchange" class="col-sm-4 control-label">Force password change</label>
                                                                <input type="checkbox" style="margin: 10px 0 0"  name="preference_auth_forcepasswordchange" id ="id_preference_auth_forcepasswordchange" {% if pref.value %}checked{% endif %}>
                                                            </div>
                                                        </li>
                                                        <li class="list-group-item">
                                                            <div class="form-group">
                                                                <label for="id_firstname" class="col-sm-4 control-label">Firstname<span style='color:#d11010; font-size: large'>*</span> </label>
                                                                <div class="col-ms-5" style="display: table; width:40%">
                                                                    {#<span class="input-group-addon"><i class="fa fa-edit"></i></span>#}
                                                                    <input type="text" class="form-control" style="margin-right: 15px" name="firstname" id="id_firstname" autocomplete="off"
                                                                           value="{{mdluser.firstname}}" placeholder="Please enter the firstname">
                                                                </div>
                                                            </div>
                                                        </li>
                                                        <li class="list-group-item">
                                                            <div class="form-group">
                                                                <label for="id_lastname" class="col-sm-4 control-label">Surname<span style='color:#d11010; font-size: large'>*</span> </label>
                                                                <div class="col-ms-5" style="display: table; width:40%">
                                                                    {#<span class="input-group-addon"><i class="fa fa-edit"></i></span>#}
                                                                    <input type="text" class="form-control" style="margin-right: 15px" name="lastname" id="id_lastname" autocomplete="off"
                                                                           value="{{mdluser.lastname}}" placeholder="Please enter the surname">
                                                                </div>
                                                            </div>
                                                        </li>
                                                        <li class="list-group-item">
                                                            <div class="form-group">
                                                                <label for="id_email" class="col-sm-4 control-label">Email address<span style='color:#d11010; font-size: large'>*</span> </label>
                                                                <div class="col-ms-5" style="display: table; width:40%">
                                                                    {#<span class="input-group-addon"><i class="fa fa-edit"></i></span>#}
                                                                    <input type="text" class="form-control" style="margin-right: 15px" name="email" id="id_email" autocomplete="off"
                                                                           value="{{mdluser.email}}" placeholder="Please enter the email address">
                                                                </div>
                                                            </div>
                                                        </li>
                                                        <li class="list-group-item">
                                                            <div class="form-group">
                                                                <label for="id_maildisplay" class="col-sm-4 control-label">Email display</label>
                                                                <div class="input-group" style="width: 40%">
                                                                    <span class="input-group-addon"><i class="fa fa-language"></i></span>
                                                                    <select id="id_maildisplay" class='form-control select2' name="maildisplay" >
                                                                        {% for option, label in mail_dis %}
                                                                            <option value="{{option}}" {% if (option == mdl.maildisplay) %}selected{% endif %}>{{label}}</option>
                                                                        {% endfor %}
                                                                    </select>
                                                                </div>
                                                            </div>
                                                        </li>
                                                        <li class="list-group-item">
                                                            <div class="form-group">
                                                                <label for="id_city" class="col-sm-4 control-label">City/town</label>
                                                                <div class="input-group" style="width: 40%">
                                                                    <span class="input-group-addon"><i class="fa fa-edit"></i></span>
                                                                    <input type="text" class="form-control" name="city" id="id_city"  autocomplete="off" value="{{mdluser.city}}" placeholder="Please enter the city">
                                                                </div>
                                                            </div>
                                                        </li>
                                                        <li class="list-group-item">
                                                            <div class="form-group">
                                                                <label for="id_country" class="col-sm-4 control-label">Select a country</label>
                                                                <div class="input-group" style="width: 40%">
                                                                    <span class="input-group-addon"><i class="fa fa-language"></i></span>
                                                                    <select id="id_country" class='form-control select2' name="country">
                                                                        <option value="">Select a country ...</option>
                                                                        {% for option, label in countries %}
                                                                            <option value="{{option}}" {% if (option == mdluser.country) %}selected{% endif %}>{{label}}</option>
                                                                        {% endfor %}
                                                                    </select>
                                                                </div>
                                                            </div>
                                                        </li>
                                                        <li class="list-group-item">
                                                            <div class="form-group">
                                                                <label for="id_timezone" class="col-sm-4 control-label">Timezone</label>
                                                                <div class="input-group" style="width: 40%">
                                                                    <span class="input-group-addon"><i class="fa fa-language"></i></span>
                                                                    <select id="id_timezone" class='form-control select2' name="timezone" >
                                                                        {% for option, label in timezone %}
                                                                            <option value="{{option}}" {% if (option == mdluser.timezone) %}selected{% endif %}>{{label}}</option>
                                                                        {% endfor %}
                                                                    </select>
                                                                </div>
                                                            </div>
                                                        </li>
                                                        <li class="list-group-item">
                                                            <div class="form-group">
                                                                <label for="input_title" class="col-sm-4 control-label">Description</label>
                                                                <div class="input-group" style="width: 40%">
                                                                    <textarea class="form-control" id="id_description" name="description" rows="10">{{ mdluser.description }}</textarea>
                                                                    {% block ckeditor %}
                                                                        <script src="{{ site.uri.public }}/ckeditor/ckeditor.js"></script>
                                                                        <script>CKEDITOR.replace('id_description'); </script>
                                                                    {% endblock %}
                                                                    {#<p class="help-block">This will become the new title for all users in the selected group.</p>#}
                                                                </div>
                                                            </div>
                                                        </li>
                                                    </ul>
                                                </div>
                                            </div>
                                        </div>
                                    </div>

                                    {#User picture#}
                                    {#<div class="form-group">#}
                                        {#<div class="panel-group">#}
                                            {#<div class="panel panel-default">#}
                                                {#<div class="panel-heading">#}
                                                    {#<h4 class="panel-title">#}
