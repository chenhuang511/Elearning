                                                        <li class="list-group-item">
                                                            <div class="form-group">
                                                                <label for="input_group" class="col-sm-4 control-label">List of interests</label>
                                                                <div class="input-group" style="width: 40%">
                                                                    <div class="input textarea clearfix example1"></div>
                                                                    {% block stylesheets %}
                                                                        <link href="{{ site.uri.public }}/js/tagglejs/assets/css/taggle.css" type="text/css" rel="stylesheet" />
