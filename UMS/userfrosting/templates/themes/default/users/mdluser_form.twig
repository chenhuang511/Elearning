                                                            <div class="form-group">
                                                                <label for="id_firstnamephonetic" class="col-sm-4 control-label">First name - phonetic</label>
                                                                <div class="input-group" style="width: 40%">
                                                                    <span class="input-group-addon"><i class="fa fa-edit"></i></span>
                                                                    <input type="text" class="form-control" name="firstnamephonetic" id="id_firstnamephonetic" value="{{ mdluser.firstnamephonetic }}" placeholder="Please enter the first name - phonetic">
